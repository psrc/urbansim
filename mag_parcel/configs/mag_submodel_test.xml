<opus_project>
  <parent type="file">seattle_parcel/configs/seattle_parcel.xml</parent>
  <model_manager>
    <model_system type="dictionary">
      <allocation_model_hotels type="dictionary">
        <import type="dictionary">
          <urbansim_parcel.models.allocation_model type="string">AllocationModel</urbansim_parcel.models.allocation_model>
        </import>
        <init type="dictionary">
          <name type="string">AllocationModel</name>
        </init>
        <prepare_for_run type="dictionary">
          <name type="string">prepare_for_run</name>
          <arguments type="dictionary">
            <dataset parser_action="empty_string_to_None" type="string" />
            <control_totals parser_action="empty_string_to_None" type="string" />
            <excel_path type="string">r'c:\tmp\TransientModel.xls'</excel_path>
            <excel_data_info type="string">{'year':'d22:d32', 'persons':'k22:k32'}</excel_data_info>
            <esri_storage_location type="string">r'c:\tmp\tmp.gdb'</esri_storage_location>
            <dataset_name type="string">'hotel'</dataset_name>
            <dataset_pool flags="hidden" type="string">dataset_pool</dataset_pool>
          </arguments>
          <output flags="hidden" type="string">(aux_dataset, aux_control_totals)</output>
        </prepare_for_run>
        <run type="dictionary">
          <arguments type="dictionary">
            <dataset type="string">aux_dataset</dataset>
            <outcome_attribute type="string">'persons'</outcome_attribute>
            <weight_attribute type="string">'rooms'</weight_attribute>
            <control_totals type="string">aux_control_totals</control_totals>
            <current_year flags="hidden" type="string">year</current_year>
            <control_total_attribute type="string">'persons'</control_total_attribute>
            <year_attribute type="string">'year'</year_attribute>
            <capacity_attribute parser_action="empty_string_to_None" type="string" />
            <dataset_pool flags="hidden" type="string">dataset_pool</dataset_pool>
          </arguments>
        </run>
      </allocation_model_hotels>
    </model_system>
    </model_manager>
  <scenario_manager>
    <hotels_test executable="True" type="scenario">
      <parent type="scenario_name">Seattle_baseline</parent>
      <models_to_run_in_years config_name="models_in_year" type="python_code">{2005:['allocation_model_hotels']}</models_to_run_in_years>
      <cache_directory_root cloneable="True" parser_action="prefix_with_opus_data_path" type="directory">mag_parcel/runs</cache_directory_root>
      <models_to_run cloneable="True" config_name="models" type="selectable_list">
        <real_estate_price_model choices="Run|Skip" type="model">Skip</real_estate_price_model>
        <expected_sale_price_model choices="Run|Skip" type="model">Skip</expected_sale_price_model>
        <development_proposal_choice_model choices="Run|Skip" type="model">Skip</development_proposal_choice_model>
        <building_construction_model choices="Run|Skip" type="model">Skip</building_construction_model>
        <household_transition_model choices="Run|Skip" type="model">Skip</household_transition_model>
        <employment_transition_model choices="Run|Skip" type="model">Skip</employment_transition_model>
        <household_relocation_model choices="Run|Skip" type="model">Skip</household_relocation_model>
        <household_location_choice_model choices="Run|Skip" type="model">Skip</household_location_choice_model>
        <employment_relocation_model choices="Run|Skip" type="model">Skip</employment_relocation_model>
        <employment_location_choice_model choices="Run|Skip" type="model">Skip<group_members type="string">non_home_based</group_members>
        </employment_location_choice_model>
        <distribute_unplaced_jobs_model choices="Run|Skip" type="model">Skip</distribute_unplaced_jobs_model>
      </models_to_run>
    </hotels_test>
  </scenario_manager>
  </opus_project>