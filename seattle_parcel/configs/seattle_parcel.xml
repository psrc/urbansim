        <user_name choices="Required|Optional" type="string"> Required </user_name>
        <password choices="Required|Optional" type="password"> Required </password>
        <use_environment_variables choices="Required|Optional" type="boolean"> Required
    </use_environment_variables>
      </ms_sql_server_template>
      <esri_personal_geodb_template setexpanded="False" type="db_template">
