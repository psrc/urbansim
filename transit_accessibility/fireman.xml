<project builds-to-keep="4" build-machine="oslo.cs.washington.edu">
    <copy-this-project>opus_core</copy-this-project>
    <copy-this-project>urbansim</copy-this-project>
    <copy-this-project>psrc</copy-this-project>
	<build-steps>
		<step ant-target="unit-tests" display-name="Unit Tests" html-color="yellow">
			<log-file>unit-tests</log-file>
			<fail-if-line-matches regexp="Segmentation fault" case-insensitive="true" description="Segmentation fault." />
		</step>
		<step ant-target="release" display-name="Release"  html-color="magenta" >
			<traffic-light-color three-lamp="+green+yellow" />
			<fail-if-line-matches regexp="Segmentation fault" case-insensitive="true" description="Segmentation fault." />
		</step>
	</build-steps>
</project>