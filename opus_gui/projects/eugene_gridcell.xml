  <results_manager>
    <Libraries type="group" setexpanded="True">
      <model.gridcell type="indicator_library">
        <population type="indicator">
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >GEOGRAPHY</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >urbansim.DATASET.population</expression>
        </population>
        <number_of_jobs type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >GEOGRAPHY</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >urbansim.DATASET.number_of_jobs</expression>
        </number_of_jobs>
        <industrial_sqft type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >GEOGRAPHY</valid_datasets>
          <documentation_link type="string" />
