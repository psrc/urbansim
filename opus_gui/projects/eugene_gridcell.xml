<opus_project>
  <parent type="file">urbansim_gridcell.xml</parent>
  <general>
    <description type="string" >Sample Eugene project</description>
  </general>
  <data_manager>
    <script_library type="script_library" setexpanded="True">
      <script_path type="script_path" >projects.scripts</script_path>
      <esri_buffer_tool type="script_file" >esri_buffer</esri_buffer_tool>
      <postgis_buffer_tool type="script_file" >postgis_buffer</postgis_buffer_tool>
      <raw_sql_query type="script_file" >raw_sql</raw_sql_query>
    </script_library>
    <script_batch1 type="script_batch" setexpanded="True">
      <roads_buffer_tool type="script_config" >
        <script_hook type="script_library_ref" >esri_buffer_tool</script_hook>
        <input_shapefile type="string" >c:\data\roads.shp</input_shapefile>
        <output_shapefile type="string" >c:\data\roads_buffer.shp</output_shapefile>
        <buffer_size type="integer" >49</buffer_size>
      </roads_buffer_tool>
      <roads_sql_query type="script_config" >
        <script_hook type="script_library_ref" >raw_sql_query</script_hook>
        <database_config type="string" >blahblahblah</database_config>
        <query type="string" >update some_table set some_field = blahblahblah</query>
      </roads_sql_query>
    </script_batch1>
    <script_batch2 type="script_batch" setexpanded="True">
      <script_path type="script_path" >projects.scripts</script_path>
      <processing_script_1 type="script_file" >test</processing_script_1>
    </script_batch2>
    <documentation type="documentation_path" >documentation/installation/index.html</documentation>
  </data_manager>
  <data_manager_dbstree>
    <database_library type="database_library" setexpanded="True">
      <urbansim_db1 type="db_connection" setexpanded="True">
        <host_name type="string">urbanserver</host_name>
        <protocol type="string">xxx_protocol</protocol>
        <user_name type="string">pwaddell</user_name>
        <password type="password">sunshine</password>
      </urbansim_db1>
      <urbansim_db2 type="db_connection" setexpanded="True">
        <host_name type="string">urbanserver2</host_name>
        <protocol type="string">xxx_protocol2</protocol>
        <user_name type="string">pwaddell2</user_name>
        <password type="password">sunshine2</password>
      </urbansim_db2>
    </database_library>
  </data_manager_dbstree>
