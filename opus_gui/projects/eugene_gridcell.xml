        </industrial_sqft>
        <total_land_value type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >GEOGRAPHY</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >urbansim.DATASET.total_land_value</expression>
        </total_land_value>
        <population_density type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >gridcell</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >urbansim.DATASET.population_density</expression>
        </population_density>
      </model.gridcell>
      <eugene.frank.indicators type="indicator_library">
        <global_population_density type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >alldata</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >alldata.aggregate_all(urbansim.gridcell.population_density, function=sum)</expression>
        </global_population_density>
      </eugene.frank.indicators>
      <my_indicators append_to="True" type="indicator_library">
        <residential_units type="indicator" >
          <package type="string" >urbansim</package>
          <valid_datasets type="string" >alldata</valid_datasets>
          <documentation_link type="string" />
          <expression type="string" >alldata.aggregate_all(urbansim.gridcell.residential_units, function=sum)</expression>
        </residential_units>
      </my_indicators>
    </Libraries>
