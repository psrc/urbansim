<opus_project>
  <general>
    <description type="string" >Sample Eugene project</description>
  </general>
  <data_manager>
    <script_batch type="script_batch" >
      <script_path type="script_path" >projects.scripts</script_path>
      <processing_script_1 type="script_file" >test</processing_script_1>
    </script_batch>
  </data_manager>
  <scenario_manager>
    <default_scenario type="scenario" >
      <import type="file" >urbansim_gridcell.xml</import>
    </default_scenario>
    <Eugene_baseline type="scenario" executable="True" >
      <parent parser_action="parent" type="scenario_name" >default_scenario</parent>
      <base_year type="integer" >1980</base_year>
      <years_to_run config_name="years" type="tuple" >
        <startyear type="integer" >1981</startyear>
        <endyear type="integer" >1981</endyear>
      </years_to_run>
      <models_to_run config_name="models" type="selectable_list" >
        <prescheduled_events type="model" choices="Run|Skip" >Run</prescheduled_events>
        <events_coordinator type="model" choices="Run|Skip" >Run</events_coordinator>
        <residential_land_share_model type="model" choices="Run|Skip" >Run</residential_land_share_model>
        <land_price_model type="model" choices="Run|Skip" >Run</land_price_model>
        <development_project_transition_model type="model" choices="Run|Skip" >Run</development_project_transition_model>
        <residential_development_project_location_choice_model type="model" choices="Run|Skip" >Run</residential_development_project_location_choice_model>
        <commercial_development_project_location_choice_model type="model" choices="Run|Skip" >Run</commercial_development_project_location_choice_model>
        <industrial_development_project_location_choice_model type="model" choices="Run|Skip" >Run</industrial_development_project_location_choice_model>
        <development_event_transition_model type="model" choices="Run|Skip" >Run</development_event_transition_model>
        <events_coordinator type="model" choices="Run|Skip" >Run</events_coordinator>
        <residential_land_share_model type="model" choices="Run|Skip" >Run</residential_land_share_model>
        <household_transition_model type="model" choices="Run|Skip" >Run</household_transition_model>
        <employment_transition_model type="model" choices="Run|Skip" >Run</employment_transition_model>
        <household_relocation_model type="model" choices="Run|Skip" >Run</household_relocation_model>
        <household_location_choice_model type="model" choices="Run|Skip" >Run</household_location_choice_model>
        <employment_relocation_model type="model" choices="Run|Skip" >Run</employment_relocation_model>
        <employment_location_choice_model type="model" choices="Run|Skip" >Run<group_members type="string" >_all_</group_members>
        </employment_location_choice_model>
        <distribute_unplaced_jobs_model type="model" choices="Run|Skip" >Run</distribute_unplaced_jobs_model>
      </models_to_run>
      <baseyear_database_configuration config_name="input_configuration" type="class" >
        <Class_name flags="hidden" type="string" >DatabaseConfiguration</Class_name>
        <Class_path flags="hidden" type="string" >opus_core.database_management.database_configuration</Class_path>
        <protocol type="string" />
        <host_name type="string" />
        <user_name type="string" />
        <password type="password" />
        <use_environment_variables type="boolean" choices="True|False" >True</use_environment_variables>
        <database_name type="string" >eugene_1980_baseyear</database_name>
      </baseyear_database_configuration>
      <cache_directory parser_action="prefix_with_urbansim_cache" type="directory" >eugene/eugene_1980_baseyear_cache</cache_directory>
      <creating_baseyear_cache_configuration type="class" >
        <Class_name flags="hidden" type="string" >CreatingBaseyearCacheConfiguration</Class_name>
        <Class_path flags="hidden" type="string" >urbansim.configurations.creating_baseyear_cache_configuration</Class_path>
        <cache_directory_root parser_action="prefix_with_urbansim_cache" type="directory" >eugene/runs</cache_directory_root>
        <cache_from_mysql type="boolean" choices="True|False" >False</cache_from_mysql>
        <baseyear_cache type="class" >
          <Class_name flags="hidden" type="string" >BaseyearCacheConfiguration</Class_name>
          <Class_path flags="hidden" type="string" >opus_core.configurations.baseyear_cache_configuration</Class_path>
          <existing_cache_to_copy parser_action="prefix_with_urbansim_cache" type="directory" >eugene/eugene_1980_baseyear_cache</existing_cache_to_copy>
        </baseyear_cache>
        <cache_scenario_database type="string" >urbansim.model_coordinators.cache_scenario_database</cache_scenario_database>
        <tables_to_cache type="selectable_list" >
          <annual_employment_control_totals type="table" choices="Cache|Skip" >Cache</annual_employment_control_totals>
          <annual_household_control_totals type="table" choices="Cache|Skip" >Cache</annual_household_control_totals>
          <buildings type="table" choices="Cache|Skip" >Cache</buildings>
          <building_types type="table" choices="Cache|Skip" >Cache</building_types>
          <development_event_history type="table" choices="Cache|Skip" >Cache</development_event_history>
          <gridcells type="table" choices="Cache|Skip" >Cache</gridcells>
          <households type="table" choices="Cache|Skip" >Cache</households>
          <job_building_types type="table" choices="Cache|Skip" >Cache</job_building_types>
          <jobs type="table" choices="Cache|Skip" >Cache</jobs>
          <travel_data type="table" choices="Cache|Skip" >Cache</travel_data>
          <zones type="table" choices="Cache|Skip" >Cache</zones>
          <counties type="table" choices="Cache|Skip" >Cache</counties>
          <commercial_development_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</commercial_development_location_choice_model_coefficients>
          <commercial_development_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</commercial_development_location_choice_model_specification>
          <commercial_employment_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</commercial_employment_location_choice_model_coefficients>
          <commercial_employment_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</commercial_employment_location_choice_model_specification>
          <home_based_employment_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</home_based_employment_location_choice_model_specification>
          <home_based_employment_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</home_based_employment_location_choice_model_coefficients>
          <industrial_employment_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</industrial_employment_location_choice_model_coefficients>
          <industrial_employment_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</industrial_employment_location_choice_model_specification>
          <industrial_development_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</industrial_development_location_choice_model_coefficients>
          <industrial_development_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</industrial_development_location_choice_model_specification>
          <residential_development_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</residential_development_location_choice_model_coefficients>
          <residential_development_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</residential_development_location_choice_model_specification>
          <fazes type="table" choices="Cache|Skip" >Skip</fazes>
          <urbansim_constants type="table" choices="Cache|Skip" >Cache</urbansim_constants>
          <household_location_choice_model_coefficients type="table" choices="Cache|Skip" >Cache</household_location_choice_model_coefficients>
          <household_location_choice_model_specification type="table" choices="Cache|Skip" >Cache</household_location_choice_model_specification>
          <land_price_model_coefficients type="table" choices="Cache|Skip" >Cache</land_price_model_coefficients>
          <land_price_model_specification type="table" choices="Cache|Skip" >Cache</land_price_model_specification>
          <residential_land_share_model_coefficients type="table" choices="Cache|Skip" >Cache</residential_land_share_model_coefficients>
          <residential_land_share_model_specification type="table" choices="Cache|Skip" >Cache</residential_land_share_model_specification>
          <plan_type_group_definitions type="table" choices="Cache|Skip" >Skip</plan_type_group_definitions>
          <plan_type_groups type="table" choices="Cache|Skip" >Skip</plan_type_groups>
          <large_areas type="table" choices="Cache|Skip" >Skip</large_areas>
          <household_characteristics_for_ht type="table" choices="Cache|Skip" >Cache</household_characteristics_for_ht>
          <development_types type="table" choices="Cache|Skip" >Cache</development_types>
          <development_type_group_definitions type="table" choices="Cache|Skip" >Cache</development_type_group_definitions>
          <development_constraints type="table" choices="Cache|Skip" >Cache</development_constraints>
          <annual_relocation_rates_for_households type="table" choices="Cache|Skip" >Cache</annual_relocation_rates_for_households>
          <annual_relocation_rates_for_jobs type="table" choices="Cache|Skip" >Cache</annual_relocation_rates_for_jobs>
          <base_year type="table" choices="Cache|Skip" >Cache</base_year>
          <cities type="table" choices="Cache|Skip" >Cache</cities>
          <development_events type="table" choices="Cache|Skip" >Skip</development_events>
          <development_type_groups type="table" choices="Cache|Skip" >Cache</development_type_groups>
          <employment_adhoc_sector_group_definitions type="table" choices="Cache|Skip" >Cache</employment_adhoc_sector_group_definitions>
          <employment_adhoc_sector_groups type="table" choices="Cache|Skip" >Cache</employment_adhoc_sector_groups>
          <employment_events type="table" choices="Cache|Skip" >Cache</employment_events>
          <employment_sectors type="table" choices="Cache|Skip" >Cache</employment_sectors>
          <land_use_events type="table" choices="Cache|Skip" >Skip</land_use_events>
          <plan_types type="table" choices="Cache|Skip" >Cache</plan_types>
          <race_names type="table" choices="Cache|Skip" >Cache</race_names>
          <target_vacancies type="table" choices="Cache|Skip" >Cache</target_vacancies>
          <jobs_for_estimation type="table" choices="Cache|Skip" >Cache</jobs_for_estimation>
          <households_for_estimation type="table" choices="Cache|Skip" >Cache</households_for_estimation>
          <development_events_exogenous type="table" choices="Cache|Skip" >Skip</development_events_exogenous>
        </tables_to_cache>
        <tables_to_cache_nchunks type="dictionary" >
          <gridcells type="integer" >1</gridcells>
        </tables_to_cache_nchunks>
        <tables_to_copy_to_previous_years type="dictionary" >
          <development_type_group_definitions type="integer" >1975</development_type_group_definitions>
          <development_type_groups type="integer" >1975</development_type_groups>
          <development_types type="integer" >1975</development_types>
          <urbansim_constants type="integer" >1975</urbansim_constants>
        </tables_to_copy_to_previous_years>
      </creating_baseyear_cache_configuration>
      <dataset_pool_configuration type="class" >
        <Class_name flags="hidden" type="string" >DatasetPoolConfiguration</Class_name>
        <Class_path flags="hidden" type="string" >opus_core.configurations.dataset_pool_configuration</Class_path>
        <package_order type="list" >['eugene', 'urbansim', 'opus_core']</package_order>
        <package_order_exceptions type="dictionary" />
      </dataset_pool_configuration>
    </Eugene_baseline>
  </scenario_manager>
  
  <results_manager>
  
    <Libraries type="group">
      <model.gridcell type="indicator_library">
        <population type="indicator">
          <package type="string">urbansim</package>
          <valid_datasets type="string"></valid_datasets>
          <documentation_link type="string"></documentation_link>
          <expression type="string">urbansim.DATASET.population</expression>
        </population>
        <number_of_jobs type="indicator">
          <package type="string">urbansim</package>
          <valid_datasets type="string"></valid_datasets>
          <documentation_link type="string"></documentation_link>
          <expression type="string">urbansim.DATASET.number_of_jobs</expression>
        </number_of_jobs>
      </model.gridcell>
      
      <eugene.frank.indicators type="indicator_library">
        
      </eugene.frank.indicators>
      
      <my_indicators type="indicator_library" append_to="True">
        <residential_units type="indicator">
        <package type="string">urbansim</package>
        <valid_datasets type="string">alldata</valid_datasets>
        <documentation_link type="string"></documentation_link>
        <expression type="string">alldata.aggregate_all(urbansim.gridcell.residential_units, function=sum)</expression>
        </residential_units>
      </my_indicators>
    </Libraries>
    
    <all_source_data type="all_source_data" >
      <tunnel_scenario type="source_data" >
        <start_year type="integer" >1980</start_year>
        <end_year type="integer" >1984</end_year>
        <run_name type="string" >run1834</run_name>
      </tunnel_scenario>
      <rebuild_scenario type="source_data" >
        <start_year type="integer" >1980</start_year>
        <end_year type="integer" >1984</end_year>
        <run_name type="string" >run1835</run_name>
      </rebuild_scenario>
    </all_source_data>
    
    <results type="selectable_list" >
      <population.zone.rebuild_scenario type="indicator_result" >
        <source_data type="string" >rebuild_scenario</source_data>
        <indicator_name type="string" >population</indicator_name>
        <dataset_name type="string" >zone</dataset_name>
      </population>
      <population.zone.tunnel_scenario type="indicator_result" >
        <source_data type="string" >tunnel_scenario</source_data>
        <indicator_name type="string" >population</indicator_name>
        <dataset_name type="string" >zone</dataset_name>
      </population>
      <number_of_jobs.gridcell.rebuild_scenario type="indicator_result" >
        <source_data type="string" >rebuild_scenario</source_data>
        <indicator_name type="string" >number_of_jobs</indicator_name>
        <dataset_name type="string" >gridcell</dataset_name>
      </number_of_jobs>
      <number_of_jobs.gridcell.tunnel_scenario type="indicator_result" >
        <source_data type="string" >tunnel_scenario</source_data>
        <indicator_name type="string" >number_of_jobs</indicator_name>
        <dataset_name type="string" >gridcell</dataset_name>
      </number_of_jobs>
    </results>
    
  </results_manager>
  

  
</opus_project>
