      <model name="real_estate_price_model" type="model">
        <structure type="dictionary">
          <init type="dictionary">
            <argument name="submodel_string" parser_action="quote_string" type="string">building_use_id</argument>
            <argument config_name="outcome_attribute" name="dependent_variable" parser_action="quote_string" type="string">ln_unit_price=ln(building.unit_price)</argument>
            <argument name="filter_attribute" parser_action="quote_string" type="string">None</argument>
          </init>
          <run type="dictionary">
            <argument name="dataset" type="string">building</argument>
          </run>
          <prepare_for_estimate type="dictionary">
            <argument name="filter_variable" parser_action="quote_string" type="string">building.unit_price</argument>
          </prepare_for_estimate>
          <estimate type="dictionary">
            <argument config_name="outcome_attribute" name="dependent_variable" parser_action="quote_string" type="string">ln_unit_price=ln(building.unit_price)</argument>
            <argument name="dataset" type="string">building</argument>
          </estimate>
        </structure>
      </model>
      <model name="business_transition_model" type="model">
        <structure type="dictionary">
          <import name="import" type="dictionary">
            <class_module>urbansim.models.transition_model</class_module>
            <class_name>TransitionModel</class_name>
          </import>
          <init type="dictionary">
            <name name="name" type="string">TransitionModel</name>
            <argument name="dataset" type="string">business</argument>
            <argument name="dataset_accounting_attribute" type="string">employment</argument>
            <argument name="model_name" parser_action="quote_string" type="string">Business Transition Model</argument>
            <argument name="model_short_name" parser_action="quote_string" type="string">BTM</argument>
          </init>
          <prepare_for_run type="dictionary">
            <name name="name" type="string">prepare_for_run</name>
            <argument name="control_total_table" parser_action="quote_string" type="string">annual_business_control_totals</argument>
            <argument name="control_total_storage" type="string">base_cache_storage</argument>
          </prepare_for_run>
          <run type="dictionary">
            <argument name="year" type="string">year</argument>
            <argument name="target_attribute_name" parser_action="quote_string" type="string">total_number_of_jobs</argument>
	        <argument name="reset_dataset_attribute_value" type="string">{'building_id':-1}</argument>
          </run>
        </structure>
      </model>      
