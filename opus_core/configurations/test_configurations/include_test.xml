<?xml version='1.0' encoding='UTF-8'?>
<opus_project>
  <general>
    <project_name type="string">test_project</project_name>
  </general>
  <scenario_manager>
    <test_scenario type="scenario">
      <description type="string">a test scenario</description>
      <includeyears parser_action="include" type="path">scenario_manager/test_scenario/stuff_to_include</includeyears>
      <includexy parser_action="include" type="path">scenario_manager/test_scenario/morestuff</includexy>
      <stuff_to_include parser_action="only_for_includes" type="dictionary">
        <startyear type="integer">2000</startyear>
        <endyear type="integer">2020</endyear>
      </stuff_to_include>
      <morestuff type="dictionary">
        <x type="integer">10</x>
        <y type="integer">20</y>
      </morestuff>
    </test_scenario>
  </scenario_manager>
</opus_project>
