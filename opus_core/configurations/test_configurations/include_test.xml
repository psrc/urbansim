<opus_project>
  <xml_version>2.0</xml_version><general>
    <project_name type="string">test_project</project_name>
    <!--some comment-->
  </general>
  <scenario_manager>
    <scenario type="scenario" name="test_scenario">
      <description type="string">a test scenario</description>
      <includeyears parser_action="include" type="path">scenario_manager/scenario/stuff_to_include<!--some comment--></includeyears>
      <includexy parser_action="include" type="path">scenario_manager/scenario/morestuff<!--some comment--></includexy>
      <stuff_to_include parser_action="only_for_includes" type="dictionary">
        <!--some comment-->
        <firstyear type="integer">2000</firstyear>
        <!--some comment-->
        <lastyear type="integer">2020</lastyear>
        <!--some comment-->
      </stuff_to_include>
      <morestuff type="dictionary">
        <x type="integer">10</x>
        <!--some comment-->
        <y type="integer">20<!--some comment--></y>
      </morestuff>
      <!--some comment-->
    </scenario>
  </scenario_manager>
  <!--some comment-->
</opus_project>