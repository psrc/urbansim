<opus_project>
  <xml_version>2.0</xml_version>
  <general>
    <expression_library type="dictionary">
      <variable dataset="parcel" source="expression" type="variable_definition" use="model variable" name="ln_cost">ln(psrc.parcel.cost)</variable>
      <variable dataset="parcel" source="Python class" type="variable_definition" use="model variable" name="existing_units">urbansim_parcel.parcel.existing_units</variable>
      <variable dataset="parcel" source="Python class" type="variable_definition" use="model variable" name="bsqft">urbansim_parcel.parcel.building_sqft</variable>
    </expression_library>
    <project_name type="string">test_project</project_name>
    <test_node oceanic_attribute="squid" test_attribute="parent_value" type="string">this is just to test attribute inheritance</test_node>
  </general>
  <model_manager>
    <estimation_config type="dictionary" name="Estimation Configuration" />
    <models hidden="False" name="Models">
      <model type="model" name="real_estate_price_model">
        <structure>
        </structure>
        <specification>
          <submodel type="submodel" name="single_family_residential" submodel_id="24">
            <variables type="variable_list">ln_cost, existing_units, bsqft</variables>
          </submodel>
        </specification>
      </model>
    </models>
    <templates hidden="True" />
  </model_manager>
</opus_project>